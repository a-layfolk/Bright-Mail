/*按钮停留态*/
QPushButton:hover
{
    /*背景颜色*/
    background:grey;
}

/*按钮按下态*/
QPushButton:pressed
{
    /*背景颜色*/
    background-color:rgb(219,112,147);
    /*左内边距为3像素，让按下时字向右移动3像素*/
    padding-left:3px;
    /*上内边距为3像素，让按下时字向下移动3像素*/
    padding-top:3px;
}
LoginDlg{
color:white;
