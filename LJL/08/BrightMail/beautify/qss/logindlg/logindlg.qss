
/*按钮普通态*/
QPushButton#loginBtn
{
    /*字体为微软雅黑*/

    /*字体大小为20点*/
    font-size:12pt;
    /*字体颜色为白色*/
    color:white;
    /*背景颜色*/
 border-image:url(:/sources/beautify/img/logindlg/6.png);
    /*边框圆角半径为8像素*/
    border-radius:8px;
    border-style:outset;
}
